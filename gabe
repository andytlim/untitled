#SIGNUP

URL: /signup
TYPE: POST
REQUEST BODY: userid, password, email

URL: /user/lookup?user, email
TYPE: GET


#LOGIN
URL: /login
TYPE: POST
REQUEST BODY: username,password


#CHANNEL
URL: /channel
TYPE: POST
REQUEST BODY: accountId

URL:/channel/{channelName}
TYPE: GET

URL:/channel
TYPE: GET
REQUEST PARAM: categories, accountId

#SCHEDULE
URL:/{user}/schedule
TYPE: POST

URL:/{user}/schedule/playlists
TYPE: GET

